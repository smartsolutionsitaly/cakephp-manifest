{
	"name" : "smartsolutionsitaly/cakephp-manifest",
	"description" : "Manifest support for CakePHP",
	"keywords" : [
		"cakephp",
		"controller",
		"manifest"
	],
	"type" : "cakephp-plugin",
	"license" : "MIT",
	"require-dev" : {
        "php" : ">=7.0",
		"cakephp/cakephp" : "^3.6",
		"smartsolutionsitaly/cakephp-marketplace" : "^1.0"
	},
	"autoload" : {
		"psr-4" : {
			"SmartSolutionsItaly\\CakePHP\\Manifest\\" : "src"
		}
