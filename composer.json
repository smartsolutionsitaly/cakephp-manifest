	"autoload-dev" : {
		"psr-4" : {
			"SmartSolutionsItaly\\CakePHP\\Manifest\\Test\\" : "tests",
			"Cake\\Test\\" : "./vendor/cakephp/cakephp/tests"
		}
	},
	"homepage" : "https://github.com/smartsolutionsitaly/cakephp-manifest",
